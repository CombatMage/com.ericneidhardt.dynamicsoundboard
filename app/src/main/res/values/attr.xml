<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="IntroductionListItem">
		<attr name="action_title" format="string" />
		<attr name="action_summary" format="string" />
		<attr name="action_icon" format="reference" />
	</declare-styleable>

	<declare-styleable name="CustomEditText">
		<attr name="text_size" format="dimension" />
		<attr name="text_color" format="color" />
		<attr name="show_underscore" format="boolean" />
	</declare-styleable>

	<declare-styleable name="FabStates">
		<attr name="state_pause" format="boolean" />
	</declare-styleable>

	<declare-styleable name="DialogButtonBar">
		<attr name="text_cancle_button" format="string" />
		<attr name="text_ok_button" format="string" />
	</declare-styleable>

	<declare-styleable name="DialogBase">
		<attr name="has_title" format="boolean"/>
		<attr name="has_recycler_view" format="boolean"/>
	</declare-styleable>

</resources>